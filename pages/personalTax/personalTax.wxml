<view class="page">
  <view class="page__hd">
    <text class="page__title">请输入...</text>
  </view>
    <view class="form-box">
        <view class="row-wrap">
            <view class="label">税前工资</view>
            <view class="label-middle">
                <input class="wage" id="1" bindinput="KeyInput" maxlength="8"type="number" placeholder="请输入税前工资"value="{{wage}}"/>
            </view>
            <view class="lable-right">元</view>
        </view>
        <view class="row-wrap">
            <view class="label">社保公积金</view>
            <view class="label-middle">
                <input class="social"id="2" bindinput="KeyInput" maxlength="8" type="number"value="{{social}}"/>
            </view>
            <view class="lable-right">元</view>
        </view>
        <view class="row-wrap">
            <view class="label">起征额</view>
            <view class="label-middle">
                <input class="beginMoney" id="2"bindinput="KeyInput" maxlength="8" type="number"value="{{beginMoney}}"/>
            </view>
            <view class="lable-right">元</view>
        </view>
    </view>
    <view class="wtf" bindtap='process' >计算</view> 
</view>